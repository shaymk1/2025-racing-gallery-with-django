
{   
    "name": "my-racing-journey",
    "version": 2,
    "builds": [
      {
        "src": "main/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "staticfiles/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "main/wsgi.py"
      }
    ]
}